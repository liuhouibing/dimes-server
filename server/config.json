{
  "restApiRoot": "/api",
  "host": "localhost",
  "port": 80,
  "url": "http://localhost/",
  "swagger": {
      "protocol": "http"
  },
  "remoting": {
    "context": false,

    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
